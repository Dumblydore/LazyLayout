<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LazyLayout">
        <attr name="state" format="enum">
            <enum name="loading" value="0"/>
            <enum name="success" value="1"/>
            <enum name="error" value="-1"/>
        </attr>
        <attr name="loadingLayout" format="reference"/>
        <attr name="errorLayout" format="reference"/>
        <attr name="errorText" format="string"/>
        <attr name="progressTint" format="color"/>
        <attr name="displayRetry" format="boolean"/>
    </declare-styleable>
</resources>